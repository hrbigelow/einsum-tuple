{
  "program": [
    "indices[slice,coord] = RANDINT(0, DIMS(elem)[coord])",
    "updates[slice,f] = RANDOM()",
    "tensor[elem,f] = RANDOM()",
    "intensor[elem,f] = tensor[elem,f]",
    "tensor[indices[slice,:],f] += updates[slice,f]"
  ],
  "constraints": [
    "RANK(coord) == 1",
    "RANK(slice) < 8",
    "RANK(slice) > 0",
    "RANK(f) > 0",
    "RANK(f) < 8",
    "RANK(elem) > 0",
    "RANK(elem) < 8",
    "DIMS(coord)[0] == RANK(elem)"
  ],
  "tfcall": {
    "func": "tf.tensor_scatter_nd_add",
    "args": {
      "tensor": "intensor",
      "indices": "indices",
      "updates": "updates"
    },
    "return-value": "tensor"
  }
}

